{
    "title": "前端基础知识巩固",
    "author": "qborfy",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "疯子知识库": "https://qiubohong.github.io"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",

        "github",
        "github-buttons",
        "donate",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/qiubohong"
        },
        "github-buttons": {
            "buttons": [{
                "user": "qiubohong",
                "repo": "fedlearn",
                "type": "star",
                "size": "small",
                "count": true
                }
            ]
        },
        "donate": {
            "weixin": "./source/images/weixin.png",
            "alipay": "./source/images/alipay.png",
            "title": "",
            "button": "赞赏",
            "alipayText": "喜欢就支持一下呗~"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        }
    }
}